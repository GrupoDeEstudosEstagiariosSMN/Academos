@model Projeto
@{
    Layout = null;
}

<form id="formCadastroProjeto" onsubmit="projeto.cadastrar()">
    <label asp-for="Titulo"></label>
    <input type="text" asp-for="Titulo">
    <label asp-for="Descricao"></label>
    <textarea asp-for="Descricao" cols="30" rows="10"></textarea>
    <label asp-for="Autor"></label>
    <input type="text" asp-for="Autor">
    <label asp-for="Disciplina"></label>
    <input type="text" asp-for="Disciplina">
    <label asp-for="PrazoFinal"></label>
    <input type="date" asp-for="PrazoFinal">
    <label asp-for="Nota"></label>
    <input type="text" asp-for="Nota">

    <button type="submit" class="btn btn-success">Cadastrar</button>
    <button type="button" class="btn btn-primary" onclick="projeto.mostrarViewBuscar()">Voltar</button>
</form>